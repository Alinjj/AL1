money = float(input("Введите сумму вашего депозита: "))
per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
print('накопленные средства за год вклада в каждом из банков')
print(per_cent.keys())
deposit = []
deposit.extend([(money*float(per_cent['ТКБ'])) ,(money*float(per_cent['СКБ'])) ,(money*float(per_cent['ВТБ'])),(money*float(per_cent['СБЕР'])) ])
print(deposit)
max = (max(deposit))
print(f'Максимальная сумма заработка: {max}')
